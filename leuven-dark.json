{
  "name": "leuven",
  "type": "light",
  "colors": {
    "editor.background": "#25202a",
    "editor.foreground": "#cfccd2",
    "editorCursor.background": "#e1420b",
    "editor.selectionBackground": "#752c0b",
    "editor.selectionHighlightBackground": "#0601ffAA",
    "editor.selectionForeground": "#25202a",
    "editor.wordHighlightBackground": "#E6ECFFAA",
    "diffEditor.insertedTextBackground": "#442049AA",
    "diffEditor.removedTextBackground": "#25353eAA",
    "editor.lineHighlightBackground": "#2d2058AA",
    "editorSuggestWidget.focusHighlightForeground": "#9DDDFF",
    "editorBracketMatch.background": "#7B4B98",
    "editorBracketMatch.border": "#FFA5A5",
    "editorError.foreground": "#06dae7",
    "editorWarning.foreground": "#1056cd",
    "editorInfo.foreground": "#ed75ef",
    "list.focusHighlightForeground": "#9DDDFF"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#767283"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#837f87"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "fontStyle": "",
        "foreground": "#767283"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#06ffff"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8c8890"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#ffff0b"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#9fcb66"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#34c8d8"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#4ac964"
      }
    },
    {
      "name": "Variables: Other",
      "scope": ["variable.other.jsdoc"],
      "settings": {
        "foreground": "#8B7500"
      }
    },
    {
      "name": "Variables: Other jsdoc",
      "scope": [
        "variable.other.object",
        "variable.other.property",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#25202a"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ff996f"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type", "entity.other.inherited-class", "support.class"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9fcb66"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#06ffff"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#ff7fff"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#8c8890"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#8c8890"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {}
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#56e46f"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "HTML: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#848088"
      }
    },
    {
      "name": "HTML: Tag Inline KBD",
      "scope": "meta.tag.inline.kbd",
      "settings": {
        "foreground": "#848088"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": ["meta.tag entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#119cd0"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {}
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#ffff0b"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#ff55ff"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
